FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     sedFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         20;

deltaT          5e-5;

writeControl    adjustableRunTime;

writeInterval   0.5;

purgeWrite      0;

writeFormat     binary;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable on;

adjustTimeStep  true;

maxCo           0.9;

maxAlphaCo      0.9;

maxDeltaT       5e-5;

libs (
     "libgroovyBC.so"
     "libgroovyStandardBCs.so"
     "libmyturbulenceModels.so"
     );

