    {
        type            fixedValue;
        value           uniform 1e-6;
    }
    inandouthalf12
    {
        type            zeroGradient;
    }
    inandouthalf21
    {
        type            zeroGradient;
    }
    inandouthalf22
    {
        type            zeroGradient;
    }
    top
    {
