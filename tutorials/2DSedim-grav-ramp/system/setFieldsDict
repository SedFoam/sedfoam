FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues ( volScalarFieldValue alpha 0 );

regions 
(
      boxToCell 
      {
           box ( 0 -0.05 -0.025 ) ( 0.08 0 0.025 ) ;
           fieldValues ( volScalarFieldValue alpha 0.4); 
      }
);


