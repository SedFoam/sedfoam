FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            fixedFluxPressure;
        gradient        $internalField;
    }
    outlet
    {
        type            fixedFluxPressure;
        gradient        $internalField;
    }
    top
    {
        type            fixedFluxPressure;
        gradient        $internalField;
    }
    bottom
    {
	type 		fixedFluxPressure;
	gradient	$internalField;
    }
    backPlane
    {
        type            empty;
    }
    frontPlane
    {
        type            empty;
    }
}


// ************************************************************************* //
