gradPMEAN        gradPMEAN        [ 1 -2 -2 0 0 0 0 ] (0 -0 0 );

tilt             tilt [ 0 0 0 0 0 ] 1;

Cvm             Cvm [ 0 0 0 0 0 ] 0;                // Virtual/Added Mass coefficient

Cl              Cl [ 0 0 0 0 0 ] 0;                 // Lift force coefficient

