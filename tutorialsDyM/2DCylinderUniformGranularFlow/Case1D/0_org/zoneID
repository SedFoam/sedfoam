FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      zoneID;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"



    ".*"
    {
        type            zeroGradient;
    }
    
    inlet
    {
        type            cyclic;
    }

    outlet
    {
        type            cyclic;
    }
}

