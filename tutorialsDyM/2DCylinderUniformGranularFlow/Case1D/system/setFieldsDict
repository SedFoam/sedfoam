    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.a 0
);

regions
(
    boxToCell
    {
        box (-20 -20 -1) (20 1.086 1);
        fieldValues
        (
            volScalarFieldValue alpha.a 0.58
        );
    }
);


