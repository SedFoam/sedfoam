FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    "(walls|hole)"
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
    outlet
    {
        type            fixedValue;   //calculated;
        value           uniform 0; //$internalField;
    }

    overset
    {
        type            overset;
    }
}

