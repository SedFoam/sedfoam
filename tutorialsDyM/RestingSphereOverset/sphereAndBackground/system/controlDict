FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs            ("liboverset.so");

application     overPimpleDyMFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         0.3;

deltaT          1e-6;

//writeControl    timeStep;
//writeInterval   10;

writeControl    adjustableRunTime;
writeInterval   0.01;

purgeWrite      0;

writeFormat     ascii;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  yes;


maxCo           0.005;
maxAlphaCo           0.005;
maxDeltaT       1.e-5;

functions
{

forces1
{
    type          forcesSed;

    libs          ("libforcesSed.so");

    writeControl  timeStep;
    timeInterval  1;

    log           yes;

    patches       ("sphere");
    rho           rhoInf;     // Indicates incompressible
    log           true;
    rhoInf        970; // Redundant for incompressible
    origin (0 0 0); 
    rotation
        {
            type            axes;
            e3              (0 0 1);
            e1              (1 0 0);
        }
}





    
}


