libs            ("liboverset.so");

application     overSedDymFoam_rbgh;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         0.6;

deltaT          0.0000025;

//writeControl    timeStep;
//writeInterval   10;

writeControl    adjustable;
writeInterval   0.001;

purgeWrite      0;

writeFormat     binary;

writePrecision  10;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           0.01;
maxAlphaCo           0.01;

